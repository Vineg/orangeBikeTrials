<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module>
	<!-- only build for Chrome -->
	<inherits name='com.google.gwt.user.UserAgent'/>
	<set-property name="user.agent" value="gecko1_8"/>
	
	<set-configuration-property name="gdx.assetpath"
		value="..\motodroid-android\assets" />
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
	<inherits name='motodroid' />
	<inherits name='motodroid-logics' />

    <stylesheet src='orange.css' />
    <!--<stylesheet src='/orange.css' />-->

	<entry-point class='ru.vineg.orangeBikeFree.client.GwtLauncher' />
	
	<extend-configuration-property name="gdx.reflect.include" value="ru.vineg.orangeBikeFree.reflection" />
</module>